{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "google",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "quotes": ["error", "double"],
    "object-curly-spacing": "off",
    "indent": "off",
    "comma-dangle": "off",
    "operator-linebreak": "off",
    "valid-jsdoc": [
      "warn",
      {
        "prefer": {
          "return": "returns"
        },
        "requireReturn": false
      }
    ]
  }
}
